    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/6.4.2/mdb.min.js"></script>

<script src="https://cdn.jsdelivr.net/npm/moment@2.29.1/moment.min.js"></script>

<script>
        document.addEventListener("DOMContentLoaded", function () {
            const orderDateElements = document.querySelectorAll('.orderdate');


        orderDateElements.forEach(orderdate => {
            const date = orderdate.getAttribute('orderdate');
            var momentDate = moment(date);
            var formattedDate = momentDate.format('YY-MM-DD');
            orderdate.textContent = formattedDate;
        });
    });
</script>
</script>
